<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
             http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>AddPlayerServlet</servlet-name>
        <servlet-class>AddPlayerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddPlayerServlet</servlet-name>
        <url-pattern>/AddPlayerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>GetPlayer</servlet-name>
    <servlet-class>GetPlayerServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>GetPlayer</servlet-name>
    <url-pattern>/GetPlayer</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>DeletePlayerServlet</servlet-name>
    <servlet-class>DeletePlayerServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>DeletePlayerServlet</servlet-name>
    <url-pattern>/DeletePlayer</url-pattern>
</servlet-mapping>


</web-app>

